  author = {Biggerstaff, Matthew and Cauchemez, Simon and Reed, Carrie and Gambhir, Manoj and Finelli, Lyn},
  file = {/home/edwin/Documents/research/zotero/storage/RM3WIH4N/Biggerstaff et al. - 2014 - Estimates of the reproduction number for seasonal,.pdf},
  eprinttype = {pmid},
  eprint = {25186370},
  pmcid = {PMC4169819}
}

