  pages = {1100-1108},
  author = {Miller, Elizabeth and Hoschler, Katja and Hardelid, Pia and Stanford, Elaine and Andrews, Nick and Zambon, Maria},
  file = {/home/edwin/Documents/research/zotero/storage/ZTJIBKQN/Miller et al. - 2010 - Incidence of 2009 pandemic influenza A H1N1 infect.pdf;/home/edwin/Documents/research/zotero/storage/UEEMQJ9J/S0140673609621267.html}
