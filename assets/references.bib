  shortjournal = {Journal of Theoretical Biology},
  urldate = {2016-11-28},
  date = {2012-11-07},
  pages = {87-95},
  keywords = {Epidemic model,Weekday contact patterns,Influenza latent period,Influenza incubation period,Pandemic influenza model},
  author = {Towers, S. and Chowell, G.},
  file = {/home/edwin/Documents/research/zotero/storage/HII7CTFB/Towers and Chowell - 2012 - Impact of weekday social contact patterns on the m.pdf}
}

