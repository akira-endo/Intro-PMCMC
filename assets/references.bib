  author = {Dieleman, Jeanne and Romio, Silvana and Johansen, Kari and Weibel, Daniel and Bonhoeffer, Jan and Sturkenboom, Miriam and {VAESCO-GBS Case-Control Study Group}},
  file = {/home/edwin/Documents/research/zotero/storage/689RC6CU/Dieleman et al. - 2011 - Guillain-Barre syndrome and adjuvanted pandemic in.pdf},
