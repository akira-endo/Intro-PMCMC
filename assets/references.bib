  file = {/home/edwin/Documents/research/zotero/storage/VWDFG43D/Saenz et al. - 2010 - Dynamics of Influenza Virus Infection and Patholog.pdf;/home/edwin/Documents/research/zotero/storage/597MZUH4/3974.html},
  eprinttype = {pmid},
