  date = {2017},
  pages = {e74-e81},
  author = {Hodgson, David and Baguelin, Marc and van Leeuwen, Edwin and Panovska-Griffiths, Jasmina and Ramsay, Mary and Pebody, Richard and Atkins, Katherine E.},
  file = {/home/edwin/Documents/research/zotero/storage/RKQ9SK3P/Hodgson et al. - 2017 - Effect of mass paediatric influenza vaccination on.pdf}
}

